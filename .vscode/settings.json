{
    "java.project.sourcePaths": [
        "src",
        "server-java/src"
    ],
    //"java.project.resourcePaths": ["src/resources"],
    //"java.project.referencedLibraries": ["server-java/lib/**/*.jar"],

    "fileWatcher.commands": [
        {
          "match": "\\.java$",
          "cmd": [
            "javac",
            "-cp",
            "server-java/lib/javax.servlet-api-4.0.1.jar",
            "${file}",
            "-d",
            "server-java/web/WEB-INF/classes"
          ],
          "isAsync": true
        }
    ],
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "server-java/lib/gson-2.10.1.jar",
        "server-java/lib/jakarta.servlet-api-5.0.0.jar"
    ]
      
      
}
  

  